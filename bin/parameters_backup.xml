<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="beach_policy_compliance" displayName="Beach Policy compliance" type="double" 
						defaultValue="0.75" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="initial_percentage_of_elevated_houses" displayName="Initial percentage of elevated houses" type="double" 
						defaultValue="0.8" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="flooded_threshold" displayName="minimum number of flooded houses" type="int" 
						defaultValue="20" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="FRM_which_scenario" displayName="FRM scenario which (1, 2 or 3)" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="buildingOrdinanceFloorElevation" displayName="Building Ordinance floor elevation" type="double" 
						defaultValue="0.2" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="CN_per_house" displayName="CN per house" type="double" 
						defaultValue="0.1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="measureInterval" displayName="Measures interval (in years)" type="int" 
						defaultValue="3" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="flood_zoning_compliance" displayName="Flood Zoning compliance" type="double" 
						defaultValue="0.75" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="FRM_where_scenario" displayName="FRM scenario where (1, 2 or 3)" type="int" 
						defaultValue="2" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="distance_from_sea" displayName="Beach Policy (Distance from sea in m)" type="double" 
						defaultValue="50.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="building_ordinance_compliance" displayName="Building Ordinance compliance" type="double" 
						defaultValue="0.8" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="initialNumberOfHouseholds" displayName="Initial Number of Households" type="int" 
						defaultValue="12000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


